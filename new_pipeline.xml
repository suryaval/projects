<pipelines group="einstein-$service_name">
	<authorization>
		<view>
			<role>Go-User</role>
		</view>
		<operate>
			<role>Go-User</role>
		</operate>
		<admins>
			<role>MS-Admin</role>
		</admins>
	</authorization>
	<pipeline name="$service_name-dev" template="ms-microservice-dev">
		<params>
			<param name="service">$service_name</param>
			<param name="appFolder">$appFolder</param>
			<param name="testProject">$appFolder.Test</param>
			<param name="newmanTestFile">$service_name-newman-tests.json</param>
		</params>
		<materials>
			<git url="https://EinstBOB:Comcast17@github.comcast.com/EinsteinBOB/$service_name.git" branch="develop" dest="source" materialName="$service_name" />
			<git url="https://EinstBOB:Comcast17@github.comcast.com/EinsteinBOB/ms-deployment-manifests.git" branch="develop" dest="deployment-manifests" autoUpdate="false" materialName="deployment-manifests">
				<filter>
					<ignore pattern="**/*" />
				</filter>
			</git>
		</materials>
	</pipeline>
	<pipeline name="$service_name-qa" labeltemplate="${$service_name-dev}" isLocked="false" template="ms-microservice-qa">
		<params>
			<param name="service">$service_name</param>
			<param name="PARENT_PIPELINE">$service_name-dev</param>
		</params>
		<materials>
			<pipeline pipelineName="$service_name-dev" stageName="GenerateDroplet" materialName="$service_name-dev" />
		</materials>
	</pipeline>
</pipelines>