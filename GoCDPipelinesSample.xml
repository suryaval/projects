<pipelines group="einstein-ms-template">
    <authorization>
      <view>
        <role>Go-User</role>
      </view>
      <operate>
        <role>Go-User</role>
      </operate>
      <admins>
        <role>MS-Admin</role>
      </admins>
    </authorization>
	<pipeline name="ms-template-dev" template="ms-microservice-dev">
		<params>
			<param name="service">ms-template</param>
			<param name="appFolder">MSTemplate</param>
			<param name="testProject">MSTemplate.Test</param>
		</params>
		<materials>
			<git url="https://EinstBOB:Comcast17@github.comcast.com/EinsteinBOB/ms-template.git" branch="develop" dest="source" materialName="ms-template"/>
			<git url="https://EinstBOB:Comcast17@github.comcast.com/EinsteinBOB/ms-deployment-manifests.git" branch="develop" dest="deployment-manifests" autoUpdate="false" materialName="deployment-manifests">
				<filter>
					<ignore pattern="**/*" />
				</filter>
			</git>
		</materials>
	</pipeline>
	<pipeline name="ms-template-qa" template="ms-microservice-qa">
		<params>
			<param name="PARENT_PIPELINE">ms-template-dev</param>
			<param name="service">ms-template</param>
		</params>
		<materials>
			<pipeline pipelineName="ms-template-dev" stageName="Build.Dev" />
		</materials>
	</pipeline>
</pipelines>